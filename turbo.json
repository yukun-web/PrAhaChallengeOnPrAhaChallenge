{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["POSTGRES_URL","EVENT_BUS_AUTH_TOKEN"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    },
    "lint": {},
    "format": {},
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    }
  }
}
